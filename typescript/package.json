{
  "name": "cascaded-point-cloud-fit-ts",
  "version": "0.1.0",
  "description": "TypeScript implementation of cascaded point cloud registration using PCA and ICP",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "cli": "tsx src/cli/index.ts"
  },
  "keywords": [
    "point-cloud",
    "registration",
    "icp",
    "pca",
    "3d",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "ml-matrix": "^6.10.4",
    "kd-tree-javascript": "^1.0.3",
    "papaparse": "^5.4.1",
    "commander": "^11.1.0",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/express": "^4.17.21",
    "@types/papaparse": "^5.3.14",
    "@types/commander": "^2.12.2",
    "typescript": "^5.3.3",
    "tsx": "^4.7.0",
    "vitest": "^1.1.0",
    "@vitest/coverage-v8": "^1.1.0",
    "eslint": "^8.55.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "prettier": "^3.1.1"
  }
}

